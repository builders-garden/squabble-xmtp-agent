{
  "deploy": {
    "startCommand": "yarn start",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "build": {
    "builder": "NIXPACKS",
    "nixpacksVersion": "1.21.0"
  },
  "environments": {
    "production": {
      "variables": {
        "NODE_VERSION": "20"
      }
    }
  }
}
